{
    "name": "gemvc/core",
    "description": "Core GEMVC framework - common functionality for all web servers",
    "type": "library",
    "homepage": "https://gemvc.de",
    "require": {
        "php": ">=8.1",
        "firebase/php-jwt": "^6.8",
        "symfony/dotenv": "^7.2",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gemvc\\": "src/",
            "Gemvc\\Database\\": "src/database/",
            "Gemvc\\Database\\Query\\": "src/database/query/",
            "Gemvc\\Helper\\": "src/helper/",
            "Gemvc\\Http\\": "src/http/",
            "Gemvc\\Core\\": "src/core/",
            "Gemvc\\CLI\\": "src/CLI/",
            "Gemvc\\CLI\\Commands\\": "src/CLI/commands/"
        },
        "files": [
            "src/database/SqlEnumCondition.php"
        ]
    },
    "bin": [
        "bin/gemvc"
    ],
    "authors": [
        {
            "name": "Ali Khorsandfard",
            "email": "ali.khorsandfard@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "suggest": {
        "gemvc/swoole": "For OpenSwoole web server support",
        "gemvc/apache": "For Apache web server support", 
        "gemvc/nginx": "For Nginx web server support"
    }
}
