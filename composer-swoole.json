{
    "name": "gemvc/swoole",
    "description": "GEMVC OpenSwoole web server implementation",
    "type": "library",
    "homepage": "https://gemvc.de",
    "require": {
        "php": ">=8.1",
        "gemvc/core": "^1.1",
        "hyperf/db-connection": "^3.1",
        "hyperf/database": "^3.1",
        "hyperf/di": "^3.1",
        "hyperf/pool": "^3.1",
        "nesbot/carbon": "^2.73",
        "carbonphp/carbon-doctrine-types": "^3.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gemvc\\Swoole\\": "src/swoole/"
        }
    },
    "extra": {
        "gemvc": {
            "webserver": "swoole",
            "port": 9501,
            "features": ["websocket", "async", "hot-reload"]
        }
    },
    "authors": [
        {
            "name": "Ali Khorsandfard",
            "email": "ali.khorsandfard@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
