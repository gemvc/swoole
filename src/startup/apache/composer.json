{
    "name": "project/apache-app",
    "description": "GEMVC Apache Application",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "gemvc/swoole": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "serve": "php -S localhost:8080 -t public"
    }
}
