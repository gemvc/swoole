{
    "name": "gemvc/nginx",
    "description": "GEMVC Nginx web server implementation",
    "type": "library",
    "homepage": "https://gemvc.de",
    "require": {
        "php": ">=8.1",
        "gemvc/core": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gemvc\\Nginx\\": "src/nginx/"
        }
    },
    "extra": {
        "gemvc": {
            "webserver": "nginx",
            "port": 80,
            "features": ["php-fpm", "reverse-proxy", "static-files"]
        }
    },
    "authors": [
        {
            "name": "Ali Khorsandfard",
            "email": "ali.khorsandfard@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
