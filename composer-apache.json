{
    "name": "gemvc/apache",
    "description": "GEMVC Apache web server implementation",
    "type": "library",
    "homepage": "https://gemvc.de",
    "require": {
        "php": ">=8.1",
        "gemvc/core": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gemvc\\Apache\\": "src/apache/"
        }
    },
    "extra": {
        "gemvc": {
            "webserver": "apache",
            "port": 80,
            "features": ["traditional-php", "mod-php", "php-fpm"]
        }
    },
    "authors": [
        {
            "name": "Ali Khorsandfard",
            "email": "ali.khorsandfard@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
